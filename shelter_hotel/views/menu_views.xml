<?xml version='1.0' encoding='UTF-8'?>
<odoo>

  <record id="action_shelter_hotel" model="ir.actions.act_window">
    <field name="name">Shelter Room</field>
    <field name="binding_type">action</field>
    <field name="res_model">shelter.room</field>
    <field name="view_mode">tree,form</field>
    <field name="search_view_id" eval="False"/>
    <field name="context" eval="False"/>
    <field name="domain" eval="False"/>
    <field name="target">current</field>
    <field name="limit">80</field>
  </record>

  <menuitem
          id="menu_shelter_submain"
          name="Shelter"
          action="shelter_pet.action_shelter_pets"
          groups="stock.group_stock_user"
          sequence="10"
          parent="shelter_pet.menu_shelter_main"
  />

  <menuitem
          id="menu_shelter_hotel"
          name="Hotel"
          action="action_shelter_hotel"
          groups="stock.group_stock_user"
          sequence="20"
          parent="shelter_pet.menu_shelter_main"
  />

  <menuitem
          id="menu_shelter_room"
          name="Rooms"
          action="action_shelter_hotel"
          groups="stock.group_stock_user"
          sequence="10"
          parent="menu_shelter_hotel"
  />

</odoo>