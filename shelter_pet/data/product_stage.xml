<odoo>

    <!-- Stages -->
    <record id="product_stage_new" model="product.stage">
        <field name="name">New</field>
        <field name="code">new</field>
        <field name="sequence">1</field>

    </record>

    <record id="product_stage_avaiable" model="product.stage">
        <field name="name">Available</field>
        <field name="code">available</field>
        <field name="sequence">2</field>

    </record>

    <record id="product_stage_reserved" model="product.stage">
        <field name="name">Reserved</field>
        <field name="code">reserved</field>
        <field name="sequence">3</field>

    </record>

    <record id="product_stage_preadopt" model="product.stage">
        <field name="name">Preadoption</field>
        <field name="code">preadoption</field>
        <field name="sequence">4</field>
    </record>

    <record id="product_stage_done" model="product.stage">
        <field name="name">Done</field>
        <field name="code">done</field>
        <field name="sequence">5</field>
    </record>

</odoo>